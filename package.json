{
  "name": "website-about",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:setup": "npm run dev:users && npm run dev:developers && npm run dev:creators && npm run dev",
    "dev:users": "rimraf pages/users/index.js && cpy templates/_users/index.js pages/users/",
    "build:users": "rimraf pages/index.js && cpy templates/_users/index.js pages/ && next build",
    "dev:developers": "rimraf pages/developers/index.js && cpy templates/_developers/index.js pages/developers/",
    "build:developers": "rimraf pages/index.js && cpy templates/_developers/index.js pages/ && next build",
    "dev:creators": "rimraf pages/creators/index.js && cpy templates/_creators/index.js pages/creators/",
    "build:creators": "rimraf pages/index.js && cpy templates/_creators/index.js pages/ && next build",
    "start": "next start",
    "start:production": "next start -p $PORT",
    "checkout": "cpy pages/creators/index.js templates/_creators/ && cpy pages/developers/index.js templates/_developers/ && cpy pages/users/index.js templates/_users/"
  },
  "dependencies": {
    "cache-manager": "^3.4.0",
    "cache-manager-memory-store": "^1.1.0",
    "cpy-cli": "^3.1.1",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.1.0",
    "github-markdown-css": "^4.0.0",
    "jquery": "^3.5.1",
    "lost": "^8.3.1",
    "mini-graphiql": "^1.0.4",
    "next": "10.0.7",
    "next-i18next": "^8.0.2",
    "next-images": "^1.7.0",
    "next-seo": "^4.20.0",
    "numeral": "^2.0.6",
    "postcss": "^8.2.6",
    "postcss-custom-media": "^8.0.0",
    "react": "17.0.1",
    "react-countup": "^4.3.3",
    "react-dom": "17.0.1",
    "react-dropdown": "^1.9.2",
    "react-flags-select": "^2.1.2",
    "react-icons": "^4.2.0",
    "react-lottie": "^1.2.3",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^15.4.3",
    "react-vertical-timeline-component": "^3.3.3",
    "react-visibility-sensor": "^5.1.1",
    "remark-emoji": "^2.1.0",
    "remark-gfm": "^1.0.0",
